{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node", "cypress-file-upload"],
    "typeRoots": ["../node_modules/@types"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node"
